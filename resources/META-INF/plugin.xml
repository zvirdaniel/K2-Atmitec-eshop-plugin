<idea-plugin>
    <id>cz.k2.eshop</id>
    <name>K2 E-Shop Integration</name>
    <version>1.0</version>
    <vendor email="Daniel.Zvir@k2.cz" url="http://www.k2.cz">K2 atmitec s.r.o.</vendor>

    <description><![CDATA[
      This plugin was made for <b>internal use only</b>.<br>
    ]]></description>

    <change-notes><![CDATA[
      This is the first version. It provides references to .phtml files from GetTemplate functions.<br>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="145.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html       on how to target different products -->
    <depends>com.intellij.modules.lang</depends>
    <depends>com.jetbrains.php</depends>
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <psi.referenceContributor implementation="cz.k2.eshop.K2ReferenceContributor"/>
        <completion.contributor language="PHP" implementationClass="cz.k2.eshop.K2CompletionContributor"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>